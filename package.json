{
  "name": "typeorm-crud-generator",
  "version": "1.0.3",
  "description": "Generate CRUD and entity in NestJS project based on database table structure",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "tg": "dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "templates/**/*"
  ],
  "scripts": {
    "build": "tsc && npm run copy-templates",
    "copy-templates": "node -e \"require('fs').cpSync('src/templates', 'dist/templates', {recursive: true, force: true})\"",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "generate": "node dist/cli.js"
  },
  "keywords": [
    "nestjs",
    "typeorm",
    "crud",
    "generator",
    "entity",
    "database",
    "typeorm-crud-generator",
    "typeorm-model-generator",
    "typeorm-model-generator-inspired"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0|| ^11.0.0",
    "@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0|| ^11.0.0",
    "typeorm": "^0.3.0"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "change-case": "^4.1.2",
    "handlebars": "^4.7.8",
    "mssql": "^11.0.1",
    "mysql2": "^3.14.3",
    "oracledb": "^6.9.0",
    "pg": "^8.16.3",
    "pluralize": "^8.0.0",
    "prettier": "^3.0.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cz-emoji": "^1.3.2-canary.2",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wulianyougithub/typeorm-curd-generator.git"
  },
  "bugs": {
    "url": "https://github.com/wulianyougithub/typeorm-curd-generator/issues"
  },
  "homepage": "https://github.com/wulianyougithub/typeorm-curd-generator#readme",
  "inspiration": {
    "project": "typeorm-model-generator",
    "repository": "https://github.com/wulianyougithub/typeorm-curd-generator",
    "license": "MIT"
  },
  "config": {
    "commitizen": {
     "path": "cz-emoji"
    }
  }
}
